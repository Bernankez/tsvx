{
  "name": "@tsvx/monorepo",
  "type": "module",
  "version": "0.0.2",
  "private": true,
  "packageManager": "pnpm@7.12.0",
  "description": "TypeScript & Vue Execute (tsvx)",
  "author": {
    "name": "Cole",
    "email": "bernankez@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/bernankez/tsvx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/tsvx.git"
  },
  "bugs": "https://github.com/bernankez/tsvx/issues",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "tsx ./scripts/copy.ts",
    "clean": "rimraf packages/*/{dist,es,lib}",
    "build": "pnpm clean && pnpm -r -F \"./packages/*\" build",
    "release": "bumpp package.json packages/*/package.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.2.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "changelogithub": "^0.12.7",
    "eslint": "^8.33.0",
    "fs-extra": "^11.1.0",
    "happy-dom": "^8.2.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.6.2",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4",
    "vue": "^3.2.47"
  },
  "pnpm": {
    "patchedDependencies": {
      "unbuild@1.1.1": "patches/unbuild@1.1.1.patch"
    }
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
