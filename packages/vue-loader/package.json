{
  "name": "@tsvx/vue-loader",
  "type": "module",
  "version": "0.0.1",
  "description": "Loaders for tsvx",
  "author": "Cole <bernankez@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/tsvx/tree/master/packages/vue-loader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bernankez/tsvx.git"
  },
  "bugs": "https://github.com/bernankez/tsvx/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "tsx src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@esbuild-kit/core-utils": "^3.1.0",
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@vue/compiler-sfc": "^3.2.47",
    "esbuild": "^0.17.7",
    "uncrypto": "^0.1.2"
  }
}
